<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" prefHeight="600" prefWidth="320" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.frontline.quevie.userinterface.SearchController"
      stylesheets="@styles.css">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <ImageView fitHeight="200.0" fitWidth="200.0">
        <image>
            <Image url="@Quevie-removebg-preview.png" />
        </image>
    </ImageView>
    <Button fx:id="homeButton" onAction="#onHomeButtonClick" text="Home"/>
    <TextField fx:id="titleSearch"/>
    <Button fx:id="performSearchButton" onAction="#onPerformSearchClick" text="Search"/>
</VBox>
